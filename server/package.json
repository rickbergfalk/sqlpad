{
  "name": "sqlpad",
  "version": "4.4.0",
  "description": "Web app. Write SQL and visualize the results. Supports Postgres, MySQL, SQL Server, Crate, Vertica, SAP HANA, and BigQuery.",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "sql",
    "mssql",
    "drill",
    "postgres",
    "postgresql",
    "mysql",
    "crate",
    "presto",
    "vertica",
    "sap",
    "hana",
    "bigquery",
    "bq"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rickbergfalk/sqlpad"
  },
  "bugs": {
    "url": "https://github.com/rickbergfalk/sqlpad/issues"
  },
  "scripts": {
    "prepublishOnly": "cd .. && ./scripts/build.sh",
    "start": "node-dev server.js --config \"./config.dev.ini\" | pino-pretty",
    "test": "mocha test --timeout 10000 --recursive --exit --bail",
    "testmssql": "env SQLPAD_BACKEND_DB_URI='mssql://sa:SuperP4ssw0rd!@localhost:1433/dbname' npm run test",
    "testmysql": "env SQLPAD_BACKEND_DB_URI='mysql://root:root@localhost:3306/db2' npm run test",
    "testmariadb": "env SQLPAD_BACKEND_DB_URI='mariadb://root:password@localhost:13306/db' npm run test",
    "testpostgres": "env SQLPAD_BACKEND_DB_URI='postgres://sqlpad:sqlpad@localhost:5432/sqlpad' npm run test",
    "fixlint": "eslint --fix \"**/*.js\"",
    "lint": "eslint \"**/*.js\""
  },
  "dependencies": {
    "@google-cloud/bigquery": "^4.7.0",
    "@hapi/joi": "^17.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cassandra-driver": "^4.5.2",
    "cryptr": "^6.0.2",
    "detect-port": "^1.3.0",
    "express": "^4.17.1",
    "express-pino-logger": "^5.0.0",
    "express-session": "^1.17.1",
    "format-link-header": "^3.1.0",
    "hdb": "^0.18.0",
    "helmet": "^3.23.2",
    "ini": "^1.3.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mariadb": "^2.4.0",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "moment": "^2.27.0",
    "mssql": "^6.2.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "nedb": "^1.8.0",
    "nedb-promise": "^2.0.1",
    "node-crate": "^2.0.6",
    "node-fetch": "^2.6.0",
    "node-xlsx": "^0.15.0",
    "nodemailer": "^6.4.10",
    "papaparse": "^5.2.0",
    "passport": "^0.4.1",
    "passport-activedirectory": "^1.0.4",
    "passport-custom": "^1.1.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-saml": "^1.3.3",
    "pg": "^8.2.1",
    "pino": "^6.3.2",
    "prettier": "^2.0.5",
    "query-string": "^6.13.1",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "sanitize-filename": "^1.6.3",
    "sequelize": "^6.2.2",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.4.0",
    "snowflake-sdk": "^1.5.2",
    "socksjs": "^0.5.0",
    "sql-formatter": "^2.3.3",
    "sql-limiter": "^2.2.0",
    "sqlite3": "^4.2.0",
    "umzug": "^2.3.0",
    "uuid": "^8.2.0",
    "vertica": "^0.5.5"
  },
  "main": "./app.js",
  "bin": {
    "sqlpad": "./server.js"
  },
  "optionalDependencies": {
    "odbc": "^2.2.2"
  },
  "devDependencies": {
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.0.1",
    "ncp": "^2.0.0",
    "node-dev": "^4.0.0",
    "parse-link-header": "^1.0.1",
    "pino-pretty": "^4.0.0",
    "supertest": "^4.0.2"
  }
}
